version: "3"
services:
  web:
    build: reverse-proxy/
    volumes:
      - "./ssl:/etc/nginx/ssl"
    depends_on:
      - backend
      - frontend
      - files
      - database
    ports:
      - 443:443
  frontend:
    build: frontend/
  backend:
    build: backend/
  database:
    build: database/
  files:
    build: music-server/
